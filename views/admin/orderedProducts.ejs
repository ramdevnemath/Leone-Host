      background-color: #0056b3;
    }
  </style>

<%- include('../partials/admin/adminHeader.ejs') %>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11.3.5/dist/sweetalert2.min.css">
      <br>
      <a class="btn btn-outline-primary" onclick="history.back()">Back</a>
      <br><br>
      <div class="container-fluid">
        <div class="card">
          <div class="card-body">
            <table class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Index</th>
                  <th>Image</th>
                  <th>Product Name</th>
                  <th>Size</th>
                  <th>Quantity</th>
                  <th>Price</th>
                  <th>Type</th>
                  <th>Company</th>
                  <th>Delivery status</th>
                  <th>Order status</th>
                </tr>
              </thead>
              <tbody>
                <% order.products.forEach((element, index) => { %>
                    <tr>
                      <td><%= index + 1 %></td>
                      <td><img src="/admin/uploads/<%= orderedProductDetails[index].product.image[0] %>"  width="50"></td>
                      <td><%= orderedProductDetails[index].product.productName %></td>
                      <td><%= element.size %></td>
                      <td><%= element.quantity %></td>
                      <td>$<%= element.currentPrice %></td>
                      <td><%= orderedProductDetails[index].product.type %></td>
                      <td><%= orderedProductDetails[index].product.company %></td>
                      <td><%= element.deliverystatus %></td>
                      <td><%= element.orderstatus %></td>
                    </tr>
                  <% }) %>
              </tbody>              
          </div>
        </div>
      </div>
      <%#- include("../partials/admin/adminFooter") %>
      
  
